{
    "manifest_version": 2,
    "name": "Twitter GPT",
    "description": "Twitter GPT enables twitter to have AI",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage", "<all_urls>", "clipboardWrite"]
}
